    var uniqueVisitTimeSpan = 0;
    var waitBeforeSaveVisit = 5; //seconds
    $(function () {
        setTimeout(function() {
            // check visit uniqness
            var path = window.location.pathname;

            if (path.endsWith("/")) {
                path = path.slice(0, -1);
            }
            var dateStr = localStorage.getItem("Visit-" + path);
            var pastDate = new Date(1900,1,1);
            if (dateStr) {
                pastDate = new Date(dateStr);
            }

            var date = new Date().getTime();

            pastDate = new Date(pastDate.getTime() + uniqueVisitTimeSpan * 60000);
            if (pastDate < date) {
                // TrackVisit
               $.ajax({
                    type: "POST",
                    url: "/sitevisit",
                    data: { id: path, __RequestVerificationToken: gettoken()},
                    dataType: 'json',
                    contentType: 'application/x-www-form-urlencoded; charset=utf-8',
                    success: function () {
                        localStorage.setItem("Visit-" + path, date);
                    }
                });
            }
        }, waitBeforeSaveVisit * 1000);

        function gettoken() {
            var token = '<input name="__RequestVerificationToken" type="hidden" value="XnowacRZ-7HA2HsaY2El3z8trb3MNbI9ucfMTPJMGC8SrmV09rnRq6x_xZVPyEJzL3nzqImd0pgEiAih-XzS_6krfgQ73HTeZ0sUp8Ycr701" />';
