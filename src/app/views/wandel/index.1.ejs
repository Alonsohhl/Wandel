                <script>
                    $(document).ready(function () {

                        function getLoc() {
                            var path = window.location.pathname;
                            if (!path)
                                return "/en";
            
                            var pathItems = path.split('/');
                            if (!pathItems || pathItems.length <= 1)
                                return "/en";

                            var validLocales = ["en", "fr", "es", "pt"];
                            var validatedLocale = "en";
                            validLocales.forEach(function(l) {
                                if (l === pathItems[1])
                                    validatedLocale = pathItems[1];
                            });
                            return "/" + validatedLocale;
                        }

                        function preventPost(e) {
                            if (e.keyCode === 13) {
                                $('#searchHeaderForm').attr('action', getLoc() + "/search/");
                                $("#searchHeaderForm").submit();
                            }
                        }

                        function isEmpty(str) {
                            return (!str || 0 === str.length);
                        }

                        $("#searchHeaderBox").kendoComboBox({
                            filter: "contains",
                            autoBind: false,
                            highlightFirst: false,
                            minLength: 2,
                            noDataTemplate: '',
                            dataSource: {
                                serverFiltering: true,
                                transport: {
                                    read: {
                                        url: getLoc() + "/search/autocomplete",
                                        dataType: "json"
                                    }
                                }
                            },
                            change: function (e) {
                                var widget = e.sender;
                                if (widget.value() && widget.select() === -1) {
                                    var combobox = $("#searchHeaderBox").data("kendoComboBox");
                                    combobox.value(widget.value());
                                } else {
                                    var wrapped = widget.value();
                                    if (!isEmpty(wrapped)) {
                                        if ('"' === wrapped || !('"' === wrapped[0] && '"' === wrapped.slice(-1))) { wrapped = '"' + wrapped + '"'; }
                                        var combobox = $("#searchHeaderBox").data("kendoComboBox");
                                        combobox.value(wrapped);
                                    }
                                }
                            }
                        });
                        var searchBox = $("#searchHeaderBox").data("kendoComboBox");
                        searchBox.input.keydown(preventPost);
                        //searchBox.input.focus();
                    });
                </script>
