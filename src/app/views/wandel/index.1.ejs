					slug: link,
					max_items: 3
				},
				function (data) {
					setUpBreadcrumb(data);
				}
			);
		}
		
        //currentPage = searchForCurrentPage("Board-of-Directors", menuObject);
	}

	function setUpBreadcrumb(data) {
		if (data.items) {
			var siteBreadcrumb = $(".site-breadcrumb ul");
			breadcrumb = data.items;
			for (var i = 0; i < breadcrumb.length; i++) {
				var node = breadcrumb[i];
				if (node.Slug != "" && node.Slug != null && node.Slug != "###") {
					siteBreadcrumb.append("<li><div>/</div></li><li><a class='Akk' href='" + node.Slug + "'>" + node.Name + "</a></li>");
				}				
			}
			if (breadcrumb.length > 0 && menuHasChildrenForNode(breadcrumb[0].Name)) {
				pageIsInner = true;
			}
		}		
	}

	function menuHasChildrenForNode(node) {
		for (var i = 0; i < menuObject.children.length; i++) {
			var child = menuObject.children[i];
			if (child.title == node) {
				if (child.numberElements > 0) {
					return true;
				} else {
					return false;
				}
			}
		}
		return false;
	}

    function searchForCurrentPage(link, menu) {
        for (var i = 0; i < menu.numberElements; i++) {
            var submenu = menu.children[i];
            if (submenu.link == link) {
                return submenu;
            } else {
                var submenuPage = searchForCurrentPage(link, submenu);
                if (submenuPage != null) {
                    return submenuPage;
                }
            }
        }
        return null;
    }

    function setUpInitialMobileMenus() {
        // create initial menu for slide out pages on a mobile sized screen
        var menuDiv = $("#menuDiv");

        // final slide out panel must have menu holder for multiple slide down menus
        var menu = "<div id='slideOut1' class='slide-out-section grey-background'>" +
            "<div id='menuHolder' class='menu-holder grey-background'>" +
            "<ul id='menu1' class='left-menu mobile-menu current-menu'></ul></div>" +
            "</div>";

        menuDiv.prepend(menu);

        var backButton = "<span id='menuBackButton' style='display:none;' class='Akk mobile-back-button'><</span>"
        menuDiv.append(backButton);
        $("#menuBackButton").on('click', function () {
            // close current menu and open parent
            isAnimating = true;
            var element = $("#menu" + (currentNode.panelLevel - 1) + " li").first();
            slideUpMenuAll(currentNode.menuName, true, element);
            $("#" + currentNode.menuName).addClass('current-menu');
        });
        setUpInnerMenu(menuObject, "#menu1");
    }

    function setUpInitialDesktopMenus() {
        if (!menuObject) {
            $("#menuDiv").hide();
            return;
        }

        // create initial menu for slide out pages on a desktop sized screen
        if (menuObject.numberPanels <= 4) {
            var menuDiv = $("#menuDiv");
            for (var i = 1; i < menuObject.numberPanels; i++) {

                // alternate panel colour
                var classes = 'slide-out-section menu-panel grey-background';
                if (i % 2 == 0) {
                    classes = 'slide-out-section menu-panel white-background';
                }

                // create slideout panels and add initial menus
                var menu = "<div id='slideOut" + i + "' class='" + classes + "' value='" + i + "'>" +
                    "<ul id='menu" + i + "' class='left-menu" + ((i != 1) ? " menu-start-left'" : "'") + ">" +
                    "</ul></div>";
                menuDiv.prepend(menu);
            }

            // final slide out panel must have menu holder for multiple slide down menus
            var menu = "<div id='slideOut" + menuObject.numberPanels + "' class='slide-out-section  tx white-background' value='" + menuObject.numberPanels + "'>" +
                "<div id='menuHolder' class='menu-holder'>" +
                "<ul id='menu" + menuObject.numberPanels + "' class='left-menu inner-menu current-menu menu-start-left'></ul></div>"
            "</div>";
            menuDiv.prepend(menu);
        }

        setUpInnerMenu(menuObject, "#menu1");

    }

    function setUpEvents() {
        // Setup menu click events on start
        $("#menuToggle").change(toggleMenu);
        $('ul.left-menu li').on('click', menuSelected);
    }

    function getMenuObject(jsonObject) {
        // Parse json string to create menu

        var items = jsonObject.items;
        menuObject = createMenuNode(items[0], null, 0, 1);
        /// TODO: BUG 5/15/2017
        //if (isDesktop()) {
        //    menuObject.numberPanels = 2;
        //} else {
        //    menuObject.numberPanels = 1;
        //}

        menuObject.numberPanels = 2;
        currentNode = menuObject;
    }

	function setUpInnerMenu(element, menu) {
        // create inner menu
        var menuElement = $(menu);
        for (var i = 0; i < element.numberElements; i++) {
			var submenu = element.children[i];
			if (element.title == "Industry Sectors" ||  element.title == "SECTORES INDUSTRIALES") {
				// Industry sector home pages are instantly accessible
				var liString = "<li class='Akk' value=" + i + "><a href='" + submenu.link + "' target='" + submenu.target + "'>" + submenu.title + "</a></li>";
			}
			else if (submenu.finalElement) {
                var liString = "<li class='Akk' value=" + i + "><a href='" + submenu.link + "' target='" + submenu.target + "'>" + submenu.title + "</a></li>";
            } else {
                var liString = "<li class='Akk' value=" + i + "><a>" + submenu.title + "</a></li>";
            }

            menuElement.append(liString);
        }

    }

	function menuSelected(event) {
		if (event.ctrlKey) {
			return;
		}
        // main menu is selected. clear second menu page and reset
        if (isAnimating) {
            return;
        }

        var element = $(this);
        if (isDesktop()) {
            desktopMenuSelected(element);
        } else {
            mobileMenuSelected(element);
        }
	}

	function getMobileHeight(element) {
		var height = element.outerHeight();
		if (currentNode.parent != null) {
			$("#menuBackButton").css("display", "block")
			if (currentNode.parent.parent != null) {
				height = hideParentMenu();
			}
		}
		return height;
	}

	function hideParentMenu() {
		
		if (currentNode.parent.parent != null) {
			var height = $("#" + currentNode.parent.parent.menuName + " li").first().outerHeight();
			$("#" + currentNode.parent.parent.menuName).css("transform", "translateY(-" + (currentNode.panelLevel - 2) * height + "px)");
		} else {
			var height = $("#" + currentNode.parent.menuName + " li").first().outerHeight();
		}       
        $("#" + currentNode.parent.menuName).css("transform", "translateY(-" + (currentNode.panelLevel - 2) * height + "px)");
        return height;
	}

	function mobileMenuSelected(element) {

		if (isAnimating) {
			return;
		}
		var parent = element.parent();
		if (parent.attr('id') == currentNode.menuName) {
			// clicked on current menu - expand a new menu or go to page
			currentNode = currentNode.children[element.attr('value')];
			if (!currentNode.finalElement) {
				// open new menu
				height = getMobileHeight(element);
				isAnimating = true;
				slideUpMenuMost(element, onSlideUpCompletedMobile, height);

			} else {
				// open page and reset menu
				clearMenu();
				if (currentNode.target == "_blank") {
					window.open(currentNode.link)
					currentNode = menuObject;
				}
				else {
					window.location.href = currentNode.link;
				}
				return;
			}
		} else if (currentNode.link != "") {
			if (currentNode.target == "_blank") {
				window.open(currentNode.link);
			} else {
				window.location.href = currentNode.link;
			}
		}
	}

    function desktopMenuSelected(selectedElement) {

        var parentDiv = selectedElement.parent().parent();
        var panelLevel = parseInt(parentDiv.attr('value'));

        if (panelLevel != currentNode.panelLevel) {
            closeExtraMenus(panelLevel);
        }

        // set currentNode to correct value
        getSelectedNode(selectedElement.attr('value'), panelLevel);

        // set active li elements
        setSelectedPanels(panelLevel, selectedElement);

        if (currentNode.finalElement) {
            // if selected element has no submenu, go to link
            clearMenu();
            if (currentNode.target == "_blank") {
                window.open(currentNode.link)
                currentNode = menuObject;
            }
            else {
                window.location.href = currentNode.link;
            }
            return;
        }

        var nextLevel = panelLevel + 1;
        $("#slideOut" + nextLevel).css('transform', 'translate(' + panelLevel * 100 + "%)");
        var currentMenu = $("#menu" + nextLevel);

        // align new menu with selected li from parent menu
        var margin = getTopMarginForMenu();
        currentMenu.css({
            'margin-top': margin
        });

        // clear any previous content from current menu
        currentMenu.empty();
        currentMenu.nextAll().remove();
        currentMenu.removeClass('immediate-parent-menu ancestor-menu');

        // add current content to current menu
        setUpInnerMenu(currentNode, "#menu" + nextLevel);

        slideMenuFromLeft(currentMenu);

        // set correct on click method depending on whether next menu is a panel or a dropdown
        if (nextLevel >= menuObject.numberPanels) {
            $('#menu' + nextLevel + ' li').on('click', function () {
                innerMenuSelected($(this));
            });


            currentMenu.addClass("current-menu");
        } else {
            $('#menu' + nextLevel + ' li').on('click', menuSelected);
        }
    }

    function slideMenuFromLeft(currentMenu) {
        // slide menu in from left
        currentMenu.addClass('menu-start-left');
        var lis = currentMenu.find('li');
        lis.each(function (i) {
            var element = $(this);
            setTimeout(function () { element.stop().animate({ left: "0%" }, 300); }, i * 900 / lis.size());
        });
    }

    function getTopMarginForMenu() {
        // aligning menu with selected li means taking into account all previous selected lis from earlier panels
        var margin = 0;
        for (var i = 0; i < selectedElements.length; i++) {
            margin += selectedElements[i].attr('value') * 55;
        }
        return margin;
    }

    function setSelectedPanels(panelLevel, selectedElement) {
        // Add and remove selected lis from the list, while updating class 'active'
        if (selectedElements[panelLevel - 1] != null) {

            selectedElements[panelLevel - 1].removeClass('active white-active');
            selectedElements[panelLevel - 1] = selectedElement;
        } else {
            selectedElements.push(selectedElement);
        }

        panelLevel % 2 != 0 ? selectedElement.addClass("active") : selectedElement.addClass("white-active");    // alternating elements are white and grey
        for (var i = panelLevel; i < selectedElements.length; i++) {
            selectedElements[i].removeClass('active white-active');
        }
        selectedElements.length = panelLevel;
    }

    function getSelectedNode(childValue, panelValue) {
        // move up the menu tree to find the correct node for the panel value
        while (currentNode.panelLevel != panelValue) {
            currentNode = currentNode.parent;
        }
        currentNode = currentNode.children[childValue];
    }

    function closeExtraMenus(level) {
        // close additional menus
        var maxPanel = parseInt(menuObject.numberPanels) + 1
        for (var i = level + 2; i < maxPanel; i++) {
            $("#slideOut" + i).css('transform', 'translate(' + level * 100 + '%, 0)');
            $("#menu" + i).empty();
        }
    }

    function createMenuNode(jsonElement, parent, value, level) {
        // Create tree/node object to keep track of open menu
        var node = { title: jsonElement.Heading, parent: parent, numberElements: jsonElement.ChildCount, value: value, menuName: "menu" + level };
        node.panelLevel = level;
        node.link = jsonElement.Link;
        if (jsonElement.Target) {
            node.target = jsonElement.Target;
        } else {
            node.target = "";
        }
        if (jsonElement.ChildCount > 0) {
            node.finalElement = false;

            node.children = []
            for (var i = 0; i < node.numberElements; i++) {
                node.children.push(createMenuNode(jsonElement.Children[i], node, i, level + 1));
            }
        } else {
            node.finalElement = true;
        }
        return node;
    }

    function openMenu() {

        $("#whiteLogo").addClass("white-logo-gone");
        $("nav.webilizeHeader").addClass("slide-menu-open");

        // open initial slide out panel
        for (var i = 1; i < menuObject.numberPanels + 1; i++) {
            $("#slideOut" + i).css("transform", "translate(0,0)");
		}
		if (pageIsInner) {
			openMenuToCurrentLocation();
		}
	}

	function openMenuToCurrentLocation() {
		if (isDesktop()) {
			openDesktopMenuToCurrentLocation();
		} else {
			openMobileMenuToCurrentLocation();
		}
	}

	function openDesktopMenuToCurrentLocation() {
		var firstMenuItemIndex = getMenuItemIndex(breadcrumb[0].Name, menuObject);
		if (breadcrumb.length > 1 && firstMenuItemIndex != -1) {

			// Highlight first li
			$("#slideOut2").css("transform", "translateX(100%)");
			var firstElement = $("#menu1 li").eq(firstMenuItemIndex);
			firstElement.addClass("active");
			selectedElements.push(firstElement);

			// Slide out first part of menu

			// add current content to current menu
			currentNode = menuObject.children[firstMenuItemIndex];
			currentMenu = $("#menu2");

			// clear any previous content from current menu
			currentMenu.empty();
			currentMenu.nextAll().remove();
			currentMenu.removeClass('immediate-parent-menu ancestor-menu');

			// align new menu with selected li from parent menu
			var margin = getTopMarginForMenu();
			currentMenu.css({
				'margin-top': margin
			});

			var nextMenuItemIndex = getMenuItemIndex(breadcrumb[1].Name, currentNode);
			if (nextMenuItemIndex != -1 && breadcrumb.length > 2) {
				// Add just one li since there will be sub menus
				addLisToUl(nextMenuItemIndex, false);
				var nextElement = $("#menu2 li");
				nextElement.css("left", "0%");
				nextElement.addClass("active");
				currentNode = currentNode.children[nextMenuItemIndex];
				selectedElements.push(nextElement);
			} else {
				// Add all lis as this is the final level
				setUpInnerMenu(currentNode, "#menu2");
				slideMenuFromLeft(currentMenu);
				$("#menu2 li").eq(nextMenuItemIndex).addClass("previously-selected");
			}
			// Add on click method 
			$('#menu2 li').on('click', function () {
				innerMenuSelected($(this));
			});

			for (var i = 2; i < breadcrumb.length; i++) {

				// Add inner menu sections
				var level = i + 1;
				var currentMenu = $("#menu" + level);

				// Add back arrows
				$("#menuHolder").append("<span id='backArrow" + currentNode.panelLevel + "' value='" + currentNode.panelLevel + "' style= 'margin-top:-41px;' class='menu-back-arrow Akk' ><</span>")
				$("#backArrow" + currentNode.panelLevel).on("click", desktopMenuReturn);

				// Add new menu
				var newMenu = "<ul id='" + currentNode.menuName + "' class='inner-menu'></ul>";
				$(".menu-back-arrow").removeClass("current");
				if (i > 2) {
					$("#backArrow" + currentNode.panelLevel).addClass("active current");
				}
				$("#menuHolder").append(newMenu);

				nextMenuItemIndex = getMenuItemIndex(breadcrumb[i].Name, currentNode);

				// Either add one li or all lis depending on if this is the last iteration.
				if (i < breadcrumb.length - 1) {
					addLisToUl(nextMenuItemIndex, false);
					// Add on click method 
					$('#menu' + currentNode.panelLevel + ' li').on('click', function () {
						innerMenuSelected($(this));
					});
				} else {
					slideDownMenu();
					$("#menu" + currentNode.panelLevel + " li").eq(nextMenuItemIndex).addClass("previously-selected");
				}

				// Don't increase current node on final iteration
				if (i < breadcrumb.length - 1) {
					currentNode = currentNode.children[nextMenuItemIndex];
				}
			}

		} else if (firstMenuItemIndex != -1) {
			$("#menu1 li").eq(firstMenuItemIndex).addClass("previously-selected");
		}
	}

	function openMobileMenuToCurrentLocation() {
		var firstMenuItemIndex = getMenuItemIndex(breadcrumb[0].Name, menuObject);
		if (breadcrumb.length > 1 && firstMenuItemIndex != -1) {

			// Remove all extra elements from first menu and slide up
			var firstElement = $("#menu1 li").eq(firstMenuItemIndex);
			firstElement.addClass("active");
			selectedElements.push(firstElement);
			$("#menu1 li").not(firstElement).remove();
			var height = firstElement.outerHeight();
			$("#menu1").css("transform", "translateY(-" + height + "px)");
			$("#menuBackButton").css("display", "block")

			// Slide out first part of menu

			// add current content to current menu
			currentNode = menuObject.children[firstMenuItemIndex];
			currentMenu = $("#menu2");

			// clear any previous content from current menu
			currentMenu.empty();
			currentMenu.nextAll().remove();
			currentMenu.removeClass('immediate-parent-menu ancestor-menu');

			var nextElement = firstElement;
			for (var i = 1; i < breadcrumb.length; i++) {

				height = nextElement.outerHeight();

				var newMenu = "<ul id='" + currentNode.menuName + "' style='transform:translateY(-" + (currentNode.panelLevel - 2) * height + "px);' class='mobile-menu'></ul>";
				$("#menuHolder").append(newMenu);
				var nextMenuItemIndex = getMenuItemIndex(breadcrumb[i].Name, currentNode);

				if (i < breadcrumb.length - 1) {
					addLisToUl(nextMenuItemIndex, false);
					if (currentNode.panelLevel < 3) {
						$("#" + currentNode.menuName + " li").addClass("Akk");
					}

					$("#" + currentNode.menuName + " li").on('click', function () {
						mobileMenuSelected($(this));
					});
					setMobileMenuClasses();
					nextElement = $("#" + currentNode.menuName + " li");
				} else {
					slideDownMenu();
					hideParentMenu();
					$("#menu" + currentNode.panelLevel + " li").eq(nextMenuItemIndex).addClass("previously-selected");
				}
				if (i < breadcrumb.length - 1) {
					currentNode = currentNode.children[nextMenuItemIndex];
				}
			}

		} else {
			$("#menu1 li").eq(firstMenuItemIndex).addClass("previously-selected");
		}

	}

	function getMenuItemIndex(name, node) {
		for (var i = 0; i < node.numberElements; i++) {
			if (name == node.children[i].title) {
				return i;
			}
		}
		return -1;
	}

    function closeMenu() {
        // close all slide out panels
        if (menuObject) {
            for (var i = 1; i < menuObject.numberPanels + 1; i++) {
                $("#slideOut" + i).removeClass("slide-out-open");
                $("#slideOut" + i).css("transform", "translate(-105%, 0)");
                if (i != 1) {
                    $("#menu" + i).addClass("menu-start-left");
                }
            }
            currentNode = menuObject;
        }
        if (!isDesktop()) {
            // clear and reset mobile menu
            var menu1 = $("#menu1");
            menu1.removeClass("mobile-parent-menu");
            menu1.nextAll().remove();
            menu1.empty();
            setUpInnerMenu(menuObject, "#menu1");
            $('ul.left-menu li').on('click', menuSelected);
        }
        $("#whiteLogo").removeClass("white-logo-gone");
        $("nav.webilizeHeader").removeClass("slide-menu-open");
		clearMenu();
		selectedElements = [];
    }

    function toggleMenu() {
        // Open and close main menu
        if ($(this).find("input[type='checkbox']").is(':checked')) {
            openMenu();

        } else {
            closeMenu();
        }
    }

    function innerMenuSelected(selectedLi) {
        if (isAnimating) {
            return;
        }
		var parent = selectedLi.parent();
        if (parent.attr('id') == currentNode.menuName) {
            // clicked on current menu - expand a new menu or go to page
			var value = selectedLi.attr("value");
			currentNode = currentNode.children[value];
            if (!currentNode.finalElement && selectedLi.find('a').attr('href') == null) {
                // open new menu
                //var top = selectedLi.index() * 55 + (currentNode.panelLevel - menuObject.numberPanels - 1) * 55;
                if (currentNode.panelLevel >= menuObject.numberPanels + 2) {
                    $("#menuHolder").append("<span id='backArrow" + currentNode.panelLevel + "' value='" + currentNode.panelLevel + "' style= 'margin-top:-41px;' class='menu-back-arrow Akk' ><</span>")
                    $("#backArrow" + currentNode.panelLevel).on("click", desktopMenuReturn)
                }
                addNewMenu(selectedLi);

            }
			else {
                // open page and reset menu
                clearMenu();
                if (currentNode.target == "_blank") {
                    window.open(currentNode.link)
                    currentNode = menuObject;
                }
                else {
                    window.location.href = currentNode.link;
                }
            }
        } else {
            // clicked on a previous menu - open link

			while (parent.attr('id') != currentNode.parent.menuName) {
				currentNode = currentNode.parent;
			}
            if (currentNode.target == "_blank") {
                window.open(currentNode.link);
			} else {
                window.location.href = currentNode.link;
            }

        }

    }

    function desktopMenuReturn() {

        var targetMenuNumber = $(this).attr("value") - 1;
        var menuLength = currentNode.panelLevel;
        var targetMenuName = "menu" + targetMenuNumber;
        var selectedLi = $("#" + targetMenuName).children().first();
        currentNode = getNodeParent(targetMenuName, selectedLi);
        for (var i = targetMenuNumber + 1; i < parseInt(menuLength) + 1; i++) {
            $("#backArrow" + i).fadeOut(300, function () { $(this).remove(); });
        }
        $("#backArrow" + targetMenuNumber).addClass("current");
        $("#" + currentNode.menuName).addClass('current-menu');
    }

    function setMenuClasses() {
        if (isDesktop()) {
            setDesktopMenuClasses();
        } else {
            setMobileMenuClasses();
        }
    }

    function setDesktopMenuClasses() {
        // Set open menu classes to display correct position of underline and coloured text
        $("#" + currentNode.menuName).removeClass("ancestor-menu grand-parent-menu immediate-parent-menu").addClass('current-menu');
        if (currentNode.panelLevel >= menuObject.numberPanels) {
            var parent = currentNode.parent;
            var count = 0;
            while (parent != null && parent.panelLevel >= menuObject.numberPanels) {
                if (count == 0) {
                    $("#" + parent.menuName).removeClass("ancestor-menu grand-parent-menu current-menu").addClass('immediate-parent-menu');
                }
                else if (parent.panelLevel == menuObject.numberPanels) {
                    $("#" + parent.menuName).removeClass("immediate-parent-menu current-menu grand-parent-menu").addClass('ancestor-menu');
                } else {
                    $("#" + parent.menuName).removeClass("ancestor-menu immediate-parent-menu current-menu").addClass('grand-parent-menu');
                }
                count += 1;
                parent = parent.parent;
            }
        }
    }

    function setMobileMenuClasses() {
        $("#" + currentNode.menuName).removeClass('mobile-menu-back mobile-parent-menu').addClass('current-menu');
        if (currentNode.parent != null) {
            $("#" + currentNode.parent.menuName).removeClass('current-menu').addClass('mobile-parent-menu');
        }
        if (currentNode.panelLevel > 2) {
            $("#" + currentNode.parent.menuName).addClass('mobile-menu-back');
        }
    }

    function addNewMenu(selectedLi) {
        // open a new submenu
        isAnimating = true;
        slideUpMenuMost(selectedLi, onSlideUpCompletedDesktop, selectedLi.outerHeight());
	}

	function addLisToUl(index, animated) {
		var submenu = currentNode.children[index];

		if (submenu.finalElement) {

			var liString = "<li value=" + index + "><a href='" + submenu.link + "' target='" + submenu.target + "'>" + submenu.title + "</a></li>";
		} else {
			var liString = "<li value=" + index + "><a>" + submenu.title + "</a></li>";
		}
		if (animated) {
			$(liString).hide().prependTo("#" + currentNode.menuName).slideDown("slow");
		} else {
			$(liString).prependTo("#" + currentNode.menuName);
		}		
	}

    function slideDownMenu() {
        // slide a new menu down from header
        var count = currentNode.numberElements - 1;
        while (count > -1) {
			addLisToUl(count, true);
            count -= 1;
        }

        // set on click method
        if (isDesktop()) {
            $("#" + currentNode.menuName + " li").on('click', function () {
                innerMenuSelected($(this));
            });
            setMenuClasses();
        } else {
            if (currentNode.panelLevel < 3) {
                $("#" + currentNode.menuName + " li").addClass("Akk");
            }

            $("#" + currentNode.menuName + " li").on('click', function () {
                mobileMenuSelected($(this));
            });
            setMobileMenuClasses();
        }

        isAnimating = false;
    }

    function slideUpMenuAll(menuName, final, selectedLi) {
        // Close an open submenu by sliding up into header
        var siblings = $("#" + menuName + " li");

        var height = siblings.first().outerHeight();
        siblings.each(function (i) {
            var totalHeight = (i + 1) * height;
            var sibling = $(this);
            sibling.animate({
                now: '+=100'
            }, {
                step: function (now, fx) {
                    var move = now * totalHeight / 100;
                    sibling.css('transform', "translateY(-" + move + "px)");
                }, duration: 350, complete: function () {
                    if (i == siblings.size() - 1) {
                        // if this is the last element to complete animation, then all elements are hidden. Remove all siblings that are not selected
                        $("#" + menuName).remove();
                        if (final) {
                            // trigger slide down of new menu when longest menu has finished animating
                            if (isDesktop()) {
                                recreateMenu(selectedLi);
                            } else {
                                recreateMobileMenu(selectedLi);
                            }
                        }
                    }
                }
            });
        });
    }

    function slideUpMenuMost(selectedLi, onSlideUpCompleted, height) {
        // a new submenu is opening - we need to close the current menu except for the selected heading
        selectedLi.css({
            'z-index': '100'   // bring selected heading to the front
        });

        var siblings = $("#" + currentNode.parent.menuName + " li");
        siblings.each(function (i) {
            var totalHeight = (i) * height;     // hide all siblings behind selected heading
            $(this).animate({
                now: '+=100'
            }, {
                step: function (now, fx) {
                    var move = now * totalHeight / 100;
                    $(this).css('transform', "translateY(-" + move + "px)");
                }, duration: 250, complete: function () {
                    if (i == siblings.size() - 1) {
                        onSlideUpCompleted(siblings, selectedLi, height);
                    }
                }
            });
        });
    }

    function onSlideUpCompletedMobile(siblings, selectedLi, height) {
        // if this is the final element to finish animating then remove all unnecessary siblings,
        //  set menu classes and trigger slide down of new menu.
        siblings.not(selectedLi).remove();
        selectedLi.css('transform', 'translateY(0)');  // since this is now the only element, we don't want it translated anymore
        selectedLi.css({
            'z-index': 'auto'   // we don't want this heading to be covering anything if the menu is selected a second time
        });
        setMobileMenuClasses();
        var newMenu = "<ul id='" + currentNode.menuName + "' style='transform:translateY(-" + (currentNode.panelLevel - 2) * height + "px);' class='mobile-menu'></ul>";
        $("#menuHolder").append(newMenu);
        slideDownMenu();
    }

    function onSlideUpCompletedDesktop(siblings, selectedLi) {

        // if this is the final element to finish animating then remove all unnecessary siblings,
        //  set menu classes and trigger slide down of new menu.
        siblings.not(selectedLi).remove();
        selectedLi.css('transform', 'translateY(0)');  // since this is now the only element, we don't want it translated anymore
        selectedLi.css({
            'z-index': 'auto'   // we don't want this heading to be covering anything if the menu is selected a second time
        });
        setMenuClasses();
        var newMenu = "<ul id='" + currentNode.menuName + "' class='inner-menu'></ul>";
        $(".menu-back-arrow").removeClass("current");
        $("#backArrow" + currentNode.panelLevel).addClass("active current");
        $("#menuHolder").append(newMenu);
        slideDownMenu();
    }

    function recreateMobileMenu(selectedLi) {

        var height = selectedLi.outerHeight();
        currentNode = currentNode.parent;
        if (currentNode.parent != null) {
            height = $("#" + currentNode.parent.menuName + " li").first().outerHeight();
            $("#" + currentNode.parent.menuName).css("transform", "translateY(-" + (currentNode.panelLevel - 2) * height + "px)");
        } else {
            $("#menuBackButton").css("display", "none")
        }


        $("#" + currentNode.menuName).css("transform", "translateY(-" + (currentNode.panelLevel - 2) * height + "px)");


        var count = currentNode.numberElements - 1;
        var index = selectedLi.attr('value');
        while (count > -1) {
            var submenu = currentNode.children[count];
            if (submenu.value == index) {
                // don't make a double of the only existing element
                count -= 1;
                continue;
            }
            recreateLi(count, submenu, index, selectedLi);
            count -= 1;
        }
        $('#' + currentNode.menuName + ' li').unbind('click');
        $('#' + currentNode.menuName + ' li').on('click', function () {
            mobileMenuSelected($(this));
        });
        setMobileMenuClasses();
        isAnimating = false;
    }

    function recreateLi(count, submenu, index, selectedLi) {
        var hrefString = ""
        var classString = "";
        if (submenu.finalElement) {
            // no submenu so add link
            hrefString = "href='" + submenu.link + "' target='" + submenu.target + "'";
        }
        if (submenu.panelLevel <= 3) {
            // bold text for first two menus
            classString = "class='Akk'";
        }
        var liString = "<li " + classString + " value=" + count + "><a " + hrefString + ">" + submenu.title + "</a></li>";
        if (submenu.value > index) {
            // if element comes after existing sibling, slide down below
            $(liString).hide().insertAfter(selectedLi).slideDown("slow");;
        } else {
            // if element comes before existing sibling, slide down above
            $(liString).hide().prependTo("#" + currentNode.menuName).slideDown("slow");

        }
    }

    function recreateMenu(selectedLi) {
        // recreate parent menu after closing submenu
        var menuElement = $("#" + currentNode.menuName);
        menuElement.removeClass("immediate-parent-menu");
        menuElement.removeClass("menu-start-left");
        var count = currentNode.numberElements - 1;
        var index = selectedLi.attr('value');
        while (count > -1) {
            var submenu = currentNode.children[count];
            if (submenu.value == index) {
                // don't make a double of the only existing element
                count -= 1;
                continue;
            }
            recreateLi(count, submenu, index, selectedLi);
            count -= 1;
        }
        $('#' + currentNode.menuName + ' li').unbind('click');
        $('#' + currentNode.menuName + ' li').on('click', function () {
            innerMenuSelected($(this));
        });
        setMenuClasses();
        isAnimating = false;
    }

    function getNodeParent(menuName, selectedLi) {
        // cycle back through tree to find parent menu to reopen. Slide up all submenus.
        isAnimating = true;
        slideUpMenuAll(currentNode.menuName, true, selectedLi);
        var parent = currentNode.parent;
        $("#" + parent.menuName).removeClass("immediate-parent-menu");
        while (parent != null) {
            if (parent.menuName == menuName) {

                return parent;
            }
            slideUpMenuAll(parent.menuName, false);
            parent = parent.parent;

        }
        return null;
    }

    function clearMenu() {
        // clear all selected lis from main menu
        for (var i = 0; i < selectedElements.length; i++) {
            selectedElements[i].removeClass('active white-active');
        }
        //$("#menu2").removeClass("immediate-parent-menu ancestor-menu")
    }

</script>

<script type="text/javascript">
    $(function() {
        var pageLocales = ["en","es"];
        var currentPath = getCurrentPath(true);
        var ddlItems = [];
        var currentLocale = getLocaleFromPath();

        if (currentLocale && (currentLocale.toLowerCase() === "fr" || currentLocale.toLowerCase() === "pt")) {
            $("#headerSearch").hide();
        }

        function getCookie(cname) {
            var name = cname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }

        // items { name, locale, path }
        function setLanguageDdl(items) {
            $("#languagesDdl").html("");

            if (items.length > 0) {
                items.forEach(function(item) {
                    $("#languagesDdl")
                        .append("<li><a data-locale='" +
                            item.locale +
                            "' href='/" +
                            item.locale +
                            item.path +
                            "'>" +
                            item.name +
                            "</a></li>");
                });
            } else {
                $("#languagesDdl").remove();
            }


            $("#languagesDdl a").click(function() {
                setCookie("lang", $(this).attr("data-locale"), 365);
            });

            if(!getCookie("lang"))
                setCookie("lang", getLocaleFromPath(), 365);

            var lang = getCookie("lang") || "en";
            $("#localeButton")
                .html(
                    "  <i class=\"fa fa-map-marker\" aria-hidden=\"true\"></i>" +
                    lang.toUpperCase() +
                    "\n<i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>");

            $("#languagesDdl").find("[data-locale]").each(function() {
                if ($(this).attr("data-locale") === lang)
                    $(this).remove();
            });
        }
        
                    
        if (pageLocales) {
            pageLocales.forEach(function(s) {
                    ddlItems.push({
                        name: s.toUpperCase(),
                        locale: s,
                        path: currentPath
                    });
            });
            
            setLanguageDdl(ddlItems);
        } else {
            $.get("/api/GetLinkedPages?id=" + getCurrentPath(false),
                function(data) {
                    if (!data || !data.items)
                        return;
                    data.items.forEach(function(item) {
                        ddlItems.push({
                            name: item.Name.toUpperCase(),
                            locale: item.Locale,
                            path: "/" + item.Path
                        });
                    });

                    setLanguageDdl(ddlItems);
                });
        }

        function getCurrentPath(excludeLocale) {
            var path = window.location.pathname;
            if (!path)
                return "";

            // Example: /en/careers/jobcategories/
            // becomes ["", "en", "careers", "jobcategories"]
            var pathItems = path.split('/');
            if (!pathItems || pathItems.length === 0)
                return "";

            var subItems = [];
            if (excludeLocale && pathItems.length >= 3) {
                subItems = pathItems.slice(2, pathItems.length);
            } else if (!excludeLocale && pathItems.length >= 2) {
                subItems = pathItems.slice(1, pathItems.length);
            }

            if (subItems && subItems.length > 0) {
                var parsedPath = "/";
                subItems.forEach(function(subItem) {
                    parsedPath += subItem + "/";
                });

                return parsedPath;
            }

            return "";
        }
    });

    function getLocaleFromPath() {
        var path = window.location.pathname;
        if (!path)
            return "en";
            
        var pathItems = path.split('/');
        if (!pathItems || pathItems.length <= 1)
            return "en";

        var validLocales = ["en", "fr", "es", "pt"];
        var validatedLocale = "en";
        validLocales.forEach(function(l) {
            if (l === pathItems[1])
                validatedLocale = pathItems[1];
        });
        return validatedLocale;
    }
</script>




